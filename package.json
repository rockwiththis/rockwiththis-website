{
  "name": "rockwiththis",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start-all": "npm-run-all -p start start-server",
    "start-server": "npm run start-dev --prefix server",
    "start": "npm-run-all -p watch-css start-js",
    "start-js": "NODE_PATH=./src; react-scripts start",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "build": "npm-run-all build-css build-js",
    "build-js": "NODE_PATH=./src; react-scripts build",
    "build-css": "node-sass-chokidar src/ -o src/",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "njs-dev-all": "npm-run-all -p start-server njs-dev",
    "njs-dev": "node next-server.js",
    "njs-build": "next build",
    "njs-start": "NODE_ENV=production node next-server.js"
  },
  "dependencies": {
    "@zeit/next-sass": "^1.0.1",
    "assert": "^1.4.1",
    "howler": "^2.1.1",
    "jquery": "^3.3.1",
    "moment": "^2.22.1",
    "next": "^8.1.0",
    "next-redux-wrapper": "^3.0.0-alpha.2",
    "node-sass": "^4.12.0",
    "node-sass-chokidar": "^1.3.3",
    "nodemon": "^1.18.7",
    "npm-run-all": "^4.1.2",
    "prop-types": "^15.6.2",
    "rc-slider": "^8.6.1",
    "react": "^16.8.6",
    "react-addons-update": "^15.6.2",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.8.6",
    "react-icons": "^3.3.0",
    "react-mailchimp-form": "^1.0.1",
    "react-mailchimp-subscribe": "^1.1.0",
    "react-redux": "^6.0.0",
    "react-scripts": "2.0.5",
    "react-scroll": "^1.7.10",
    "react-share": "^2.1.1",
    "react-visibility-sensor": "^3.11.0",
    "redux": "^3.7.2",
    "redux-actions": "^2.4.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "soundcloud-widget": "^0.2.1"
  },
  "devDependencies": {
    "concurrently": "^4.0.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "mixitup": "^3.2.2",
    "pm2": "^3.2.2",
    "prompt": "^1.0.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
